{
    "executionRoleArn": "arn:aws:iam::262077174210:role/role-ecs-codedeploy",
    "containerDefinitions": [
        {
            "name": "task-definition-demo",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048",
    "family": "ecs-demo"
}
