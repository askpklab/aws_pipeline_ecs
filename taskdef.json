{
    "executionRoleArn": "a:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "pk-ecs-task-definition",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048",
    "family": "pk-ecs-task-fargate"
}